{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/app.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "env": {
        "VITE_SLUG": "bntu",
        "VITE_GRAPH_URL": "https://graph.zubr.life/v1/graphql"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "env": {
        "VITE_SLUG": "bntu",
        "VITE_GRAPH_URL": "https://graph.zubr.life/v1/graphql"
      }
    }
  ]
}
